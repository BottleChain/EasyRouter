<?xml version="1.0" encoding="utf-8"?>
<manifest package="com.example.zane.easyrouter"
          xmlns:android="http://schemas.android.com/apk/res/android">

    <application
        android:name="com.zane.easyrouter.App"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.zane.easyrouter.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name="com.zane.easyrouter.ActivityTwo"/>
        <activity android:name="com.zane.easyrouter.ActivityThree"/>
    </application>

</manifest>